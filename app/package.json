{
    "name": "client",
    "version": "1.0.0",
    "private": true,
    "description": "A desktop App for viewing and managing data from the Embedded devices",
    "author": "Embedded Data Logger",
    "license": "MIT",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "start": "vite start",
        "serve": "vite preview",
        "tauri": "tauri",
